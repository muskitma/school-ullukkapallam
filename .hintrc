{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/text-alternatives": [
      "default",
      {
        "image-alt": "off",
        "frame-title": "off"
      }
    ],
    "axe/name-role-value": [
      "default",
      {
        "link-name": "off"
      }
    ],
    "no-inline-styles": "off",
    "axe/forms": [
      "default",
      {
        "label": "off"
      }
    ],
    "button-type": "off",
    "axe/language": [
      "default",
      {
        "html-has-lang": "off"
      }
    ],
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "-webkit-text-size-adjust",
          "-webkit-text-decoration-skip"
        ]
      }
    ],
    "meta-viewport": "off"
  }
}